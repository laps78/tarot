{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jest"
    ],
    "rules": {
        "react/jsx-filename-extension": [0],
        "import/extensions": "off",
        "react/jsx-one-expression-per-line": "off",
        "import/prefer-default-export": "warn",
        "object-curly-newline": "warn",
        "no-unused-vars": "warn"
    }
}
